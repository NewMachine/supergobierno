{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false
      }
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "array-type": false,
    "object-literal-sort-keys": false,
    "no-this-assignment": false,
    "max-classes-per-file": false,
    "prefer-object-spread": false,
    "no-console": false
  },
  "jsRules": {}
}
